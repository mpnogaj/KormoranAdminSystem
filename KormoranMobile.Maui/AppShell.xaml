<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="KormoranMobile.Maui.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:KormoranMobile.Maui"
    xmlns:views="clr-namespace:KormoranMobile.Maui.Views"
    Shell.FlyoutBehavior="Flyout">

    <ShellContent
        Title="Turnieje"
        ContentTemplate="{DataTemplate views:TournamentsPage}"
        Route="TournamentsPage">
    </ShellContent>

    <ShellContent
        Title="Zaloguj"
        ContentTemplate="{DataTemplate views:LoginPage}"
        Route="TournamentsPage/LoginPage" />
    <ShellContent
        Title="Ustawienia"
        ContentTemplate="{DataTemplate views:SettingsPage}"
        Route="SettingsPage" />

</Shell>
